{
  "name": "novelous-extension",
  "private": true,
  "version": "0.0.0",
  "description": "An extension that helps publishing your novel on various Japanese novel sites.",
  "author": "IRIDE Monad <iride.monad@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/io-monad/novelous-extension",
  "repository": "io-monad/novelous-extension",
  "bugs": {
    "url": "https://github.com/io-monad/novelous-extension/issues"
  },
  "scripts": {
    "prepublish": "bower install",
    "test": "ava",
    "build": "gulp build --vendor=chrome",
    "watch": "gulp build --watch --vendor=chrome",
    "pack:chrome": "gulp pack --production --vendor=chrome",
    "pack:moz": "gulp pack --production --vendor=moz",
    "pack:opera": "gulp pack --production --vendor=opera"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "del": "^2.0.2",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.0.1",
    "gulp-cache": "^0.4.3",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^2.0.0",
    "gulp-filter": "^4.0.0",
    "gulp-git": "^1.2.4",
    "gulp-if": "^2.0.0",
    "gulp-json-transform": "^0.3.0",
    "gulp-less": "^3.0.5",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-plumber": "^1.0.1",
    "gulp-sequence": "^0.4.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-tag-version": "^1.3.0",
    "gulp-util": "^3.0.5",
    "gulp-zip": "^3.0.2",
    "jquery": "^2.2.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.6.1",
    "main-bower-files": "^2.11.1",
    "require-dir": "^0.3.0",
    "sinon": "^1.17.3",
    "sinon-chrome": "^1.1.2",
    "strftime": "^0.9.2",
    "vinyl-named": "^1.1.0",
    "webpack": "^1.12.14",
    "webpack-stream": "^3.1.0",
    "yargs": "^4.3.2"
  },
  "ava": {
    "files": [
      "tests/**/*-test.js"
    ],
    "source": [
      "app/scripts/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
